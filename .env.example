# OpenAI API Configuration
# Replace with your OpenAI API key from: https://platform.openai.com/api-keys
OPENAI_API_KEY=your-api-key-goes-here

# Model Configurations for PFC Agents
# Available models: gpt-4, gpt-3.5-turbo, etc.
DLPFC_MODEL=gpt-3.5-turbo
VMPFC_MODEL=gpt-3.5-turbo
OFC_MODEL=gpt-3.5-turbo
ACC_MODEL=gpt-3.5-turbo
MPFC_MODEL=gpt-3.5-turbo

# Optional: Additional Configuration Settings
# Logging level: DEBUG, INFO, WARNING, ERROR, CRITICAL
LOG_LEVEL=INFO

# Model Parameters
# Maximum tokens for API responses (1-4096)
MAX_TOKENS=500

# Temperature for model responses (0.0-1.0)
# Lower values: more focused, deterministic responses
# Higher values: more creative, diverse responses
TEMPERATURE=0.7 